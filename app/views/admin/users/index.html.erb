<h1>users</h1>
<table class="table">
	  <thead class="table-light">
    <tr>
      <th>Email</th>
	  <th>Role</th>
      <th colspan="3"></th>
    </tr>
</thead>
<tbody>
	<% @users.each do |user|%>
      <tr>
        <% unless current_user==user %>
        <td><%=user.email%></td>
        <% role=user.roles.first %>
        <td><%= role.name %></td>
        <%else%>
        <%end%>
      </tr>
   <%end%>
  </tbody>
</table>
