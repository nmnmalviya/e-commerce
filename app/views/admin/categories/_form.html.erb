<center>
<%= form_with model: [:admin, category] do |form| %>
<% if category.errors.any? %>
  <h2>Error</h2>
  <ul>
    <% category.errors.full_messages.each do |message| %>
      <li>
      <mark><%= message%></mark>
      </li>
    <%end%>
  </ul>
<%end%>
<div class="form-group">
  <%= form.hidden_field :parent_id, value: "#{category.id}" %>
    </div>
      <div class="form-group">
        <%= form.label :Add_category %><br>
        <%= form.text_field :category_name , class:"form-control",style:"width:250px;" %>
      </div>
      <div class="form-group">
        <%= form.submit %>
    </div>
  <% end %>
  
</center>